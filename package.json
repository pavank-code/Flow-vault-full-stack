{
  "name": "flowvault",
  "version": "1.0.0",
  "description": "AI-Powered Developer Context Preservation System",
  "main": "dist-electron/main/main.js",
  "author": "FlowVault Team",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && tsc -p tsconfig.main.json --watch\" && electron .\"",
    "build": "npm run build:react && npm run build:electron",
    "build:react": "vite build",
    "build:electron": "tsc -p tsconfig.main.json",
    "start": "electron .",
    "start:dev": "concurrently \"vite\" \"npm run electron:dev\"",
    "electron:dev": "wait-on http://localhost:5173 && tsc -p tsconfig.main.json && electron .",
    "pack": "electron-builder --dir",
    "dist": "npm run build && electron-builder"
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.0",
    "axios": "^1.6.2",
    "framer-motion": "^10.16.4",
    "@google/generative-ai": "^0.11.0",
    "lucide-react": "^0.263.1",
    "sql.js": "^1.8.0",
    "uuid": "^9.0.0",
    "openai": "^4.10.0"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/node": "^20.3.1",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.2",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.0.1",
    "electron": "^28.0.0",
    "electron-builder": "^24.6.3",
    "typescript": "^5.0.2",
    "vite": "^5.0.0",
    "wait-on": "^7.0.1",
    "tailwindcss": "^3.3.0",
    "postcss": "^8.4.24",
    "autoprefixer": "^10.4.14"
  }
}
